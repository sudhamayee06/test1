{
    "name": "crms",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "build": "ng build",
        "build-prod": "ng build --prod",
        "test": "ng test",
        "test-json": "ng test >> results.json",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "task-build": "ng build --base-href $npm_package_config_baseUrl",
        "task-start": "node server.js",
        "task-custom-url-start": "node server.js --base-url $npm_package_config_baseUrl",
        "task": "ng build --base-href $npm_package_config_baseUrl && node server.js",
        "task-win": "ng build --base-href %npm_package_config_baseUrl% && node server.js --base-url %npm_package_config_baseUrl%",
        "start-prev": "concurrently \"npm run serve-api\" \"npm run serve\"",
        "serve": "ng serve --proxy-config proxy.config.json",
        "serve-api": "nodemon server.js --watch server"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^5.2.11",
        "@angular/cdk": "^5.2.4",
        "@angular/common": "^5.1.0",
        "@angular/compiler": "^5.1.0",
        "@angular/core": "^5.1.0",
        "@angular/forms": "^5.1.0",
        "@angular/http": "^5.1.0",
        "@angular/material": "^5.2.4",
        "@angular/platform-browser": "^5.1.0",
        "@angular/platform-browser-dynamic": "^5.1.0",
        "@angular/router": "^5.1.0",
        "core-js": "^2.4.1",
        "hammerjs": "^2.0.8",
        "rxjs": "^5.5.6",
        "zone.js": "^0.8.19"
    },
    "devDependencies": {
        "@angular/cli": "1.5.6",
        "@angular/compiler-cli": "^5.1.0",
        "@angular/language-service": "^5.1.0",
        "@angular/material-moment-adapter": "^6.4.7",
        "@types/jasmine": "~2.8.3",
        "@types/jasminewd2": "~2.0.2",
        "@types/minimist": "^1.2.0",
        "@types/node": "~6.0.60",
        "bootstrap": "^4.1.3",
        "codelyzer": "^4.0.1",
        "concurrently": "^4.0.1",
        "jasmine-core": "~2.8.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~6.3.16",
        "karma-chrome-launcher": "~2.2.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-firefox-launcher": "^1.1.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-json-result-reporter": "^1.0.0",
        "karma-mocha": "^1.3.0",
        "karma-mocha-own-reporter": "^1.1.2",
        "karma-structured-json-reporter": "^1.0.1",
        "mocha": "~2.3.4",
        "moment": "^2.22.2",
        "nodemon": "^1.18.4",
        "npm-config": "^0.2.0",
        "protractor": "~5.1.2",
        "ts-node": "~3.2.0",
        "tslint": "~5.9.1",
        "typescript": "~2.5.3"
    },
    "config": {
        "baseUrl": "/"
    }
}
